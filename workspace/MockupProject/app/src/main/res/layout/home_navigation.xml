<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".navigation.HomeNavigation">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="10">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="9"
                android:layout_marginLeft="30dp"
                android:src="@drawable/youtlogos1"
                android:layout_gravity="bottom"/>
            <ImageView
                android:id="@+id/home_search"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:src="@drawable/search"
                android:layout_gravity="center"/>
        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="15dp"
            android:layout_weight="9">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/home_img1"
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/home_img1" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="30dp"
                    android:orientation="vertical"
                    android:weightSum="2">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:weightSum="5">
                        <LinearLayout
                            android:id="@+id/home_icon1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/camping" />\
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="캠핑"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/jeep" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="차막,노지"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/tent" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="글램핑"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon4"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/bagpack" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="백패킹"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon5"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/cooking" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="쿠킹"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:weightSum="5">
                        <LinearLayout
                            android:id="@+id/home_icon6"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/fishing" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="낚시"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon7"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/travel" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="여행"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon8"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/bicycle" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="액티비티"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon9"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/kayak" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="수중운동"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/home_icon10"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:padding="8dp">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/mountain" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="자연"
                                android:textStyle="bold"
                                android:textSize="10dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/home_linear1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <TextView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="9"
                        android:text="테마별로 즐기는 아웃도어 TV"
                        android:textSize="20dp"
                        android:layout_marginVertical="10dp"
                        android:layout_marginRight="100dp"
                        android:fontFamily="@font/ibmplexsanslight"/>

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:src="@drawable/right"
                        android:layout_gravity="center"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/home_rec1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp"/>
                <LinearLayout
                    android:id="@+id/home_linear2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="100dp"
                        android:layout_marginVertical="10dp"
                        android:layout_weight="9"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="인플루언서 인터뷰"
                                android:textSize="20dp"
                                android:fontFamily="@font/ibmplexsanslight"/>
                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/mic"/>
                        </LinearLayout>
                        <TextView
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:text="&amp; 추천 스팟, 아이템"
                            android:textSize="20dp"
                            android:fontFamily="@font/ibmplexsanslight"/>
                    </LinearLayout>
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:src="@drawable/right"
                        android:layout_gravity="center"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/home_rec2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp"/>
                <LinearLayout
                    android:id="@+id/home_linear3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="100dp"
                        android:layout_marginVertical="10dp"
                        android:layout_weight="9"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:text="뭐사지? 할땐"
                            android:textSize="20dp"
                            android:fontFamily="@font/ibmplexsanslight"/>
                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="캠핑집들이 훔펴보자"
                                android:textSize="20dp"
                                android:fontFamily="@font/ibmplexsanslight"/>
                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/eyes"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:src="@drawable/right"
                        android:layout_gravity="center"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/home_rec3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="10dp">
                    <ImageView
                        android:id="@+id/home_img2"
                        android:layout_width="match_parent"
                        android:layout_height="250dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/image1"/>
                    <TextView
                        android:id="@+id/home_tv1"
                        android:layout_width="250dp"
                        android:layout_height="wrap_content"
                        android:text="캠핑집들이 #13 카스텐 텐트 동계장박"
                        android:textStyle="bold"
                        android:textSize="20dp"
                        android:layout_gravity="bottom"
                        android:textColor="#ffffff"
                        android:fontFamily="@font/ibmplexsanslight"
                        android:layout_marginLeft="10dp"
                        android:layout_marginBottom="30dp"/>
                    <TextView
                        android:id="@+id/home_tv2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="BY minsangpark"
                        android:textSize="10dp"
                        android:layout_gravity="bottom"
                        android:textColor="#ffffff"
                        android:fontFamily="@font/ibmplexsanslight"
                        android:layout_marginLeft="10dp"
                        android:layout_marginBottom="20dp"/>
                </FrameLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="30dp">
                    <TextView
                        android:id="@+id/home_tv3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Style #13"
                        android:textSize="10dp"
                        android:layout_gravity="bottom"
                        android:textColor="#000000"
                        android:fontFamily="@font/ibmplexsanslight" />
                    <TextView
                        android:id="@+id/home_tv4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="· 2021.12.24"
                        android:textSize="10dp"
                        android:layout_gravity="bottom"
                        android:textColor="#676767"
                        android:fontFamily="@font/ibmplexsanslight" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/home_img3"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:src="@drawable/eye"
                            android:layout_gravity="center"/>
                        <TextView
                            android:id="@+id/home_tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="262"
                            android:textSize="10dp"
                            android:layout_gravity="bottom"
                            android:textColor="#000000"
                            android:layout_marginLeft="240dp"
                            android:fontFamily="@font/ibmplexsanslight" />
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="#EAEAEA"
                    android:paddingVertical="30dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="100dp"
                        android:layout_marginVertical="10dp"
                        android:layout_weight="9"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/home_tv6"
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:text="2022 새해는 이곳에서!"
                            android:textSize="20dp"
                            android:fontFamily="@font/ibmplexsanslight"/>
                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/home_tv7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="일출 맛집 차박지 10곳"
                                android:textSize="20dp"
                                android:fontFamily="@font/ibmplexsanslight"/>
                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/sunset"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/home_rec4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp"/>
                    <Button
                        android:id="@+id/home_btn1"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:text="장소 더보기"
                        android:layout_gravity="center"
                        android:textColor="#000000"
                        android:background="@drawable/button_round"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/home_linear4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="100dp"
                        android:layout_marginVertical="10dp"
                        android:layout_weight="9"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="아웃도어 랜선 여행"
                                android:textSize="20dp"
                                android:fontFamily="@font/ibmplexsanslight"/>
                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/camera"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:src="@drawable/right"
                        android:layout_gravity="center"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/home_rec5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="30dp"/>
                <LinearLayout
                    android:id="@+id/home_linear5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="10">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="100dp"
                        android:layout_marginVertical="10dp"
                        android:layout_weight="9"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:text="캠린이에게 필요한 정보"
                            android:textSize="20dp"
                            android:fontFamily="@font/ibmplexsanslight"/>
                        <LinearLayout
                            android:layout_width="250dp"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="여기 다 있다"
                                android:textSize="20dp"
                                android:fontFamily="@font/ibmplexsanslight"/>
                            <ImageView
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:src="@drawable/book"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:src="@drawable/right"
                        android:layout_gravity="center"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/home_rec6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</FrameLayout>