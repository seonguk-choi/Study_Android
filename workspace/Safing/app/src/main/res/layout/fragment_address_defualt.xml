<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".fragment.Address_Defualt_Fragment"
    android:background="@drawable/button_round7">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginVertical="20dp"
        android:layout_marginHorizontal="20dp"
        android:gravity="center"
        android:weightSum="7">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="수령인"
                android:textStyle="bold"
                android:layout_marginLeft="13dp"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:layout_marginRight="10dp"
                android:fontFamily="@font/ibmplexsanslight"/>
            <EditText
                android:id="@+id/address_defualt_tv1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="연락처"
                android:textStyle="bold"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="13dp"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:fontFamily="@font/ibmplexsanslight"/>
            <EditText
                android:id="@+id/address_defualt_tv2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="배송지"
                android:layout_marginLeft="13dp"
                android:textStyle="bold"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:fontFamily="@font/ibmplexsanslight"/>
            <EditText
                android:id="@+id/address_defualt_tv3"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
            <Button
                android:id="@+id/address_defualt_btn1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="주소찾기"
                android:layout_marginLeft="10dp"
                android:textStyle="bold"
                android:fontFamily="@font/nanumgothic"
                android:textColor="#F9F9FB"
                android:backgroundTint="#735340"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="              "
                android:textStyle="bold"
                android:layout_marginLeft="13dp"
                android:gravity="center_vertical"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
            <EditText
                android:id="@+id/address_defualt_v4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="              "
                android:textStyle="bold"
                android:gravity="center_vertical"
                android:layout_marginLeft="13dp"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
            <EditText
                android:id="@+id/address_defualt_tv5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:fontFamily="@font/ibmplexsanslight"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="배송메모"
                android:textStyle="bold"
                android:layout_marginRight="10dp"
                android:gravity="center_vertical"
                android:textColor="#000000"
                android:fontFamily="@font/ibmplexsanslight"/>
            <EditText
                android:id="@+id/address_defualt_tv6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#8C8A8A"
                android:backgroundTint="#85897B"
                android:fontFamily="@font/ibmplexsanslight"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_marginVertical="20dp">
            <CheckBox
                android:id="@+id/address_defualt_box1"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:buttonTint="#735340"
                android:fontFamily="@font/nanumgothic"
                android:scaleX="1.3"
                android:scaleY="1.3"
                android:textSize="15dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:text="기본 배송지로 설정"
                android:textColor="#000000"
                android:fontFamily="@font/nanumgothic"
                android:layout_marginLeft="5dp"
                android:layout_marginVertical="5dp"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>