	<!-- Portfolio Section-->
	<section class="page-section portfolio" id="portfolio">
		<div class="container">
			<h1
				class="page-section-heading text-center text-uppercase text-black">시험</h1>

			<p></p>
			<form action="grading.exam" method="post">

				<input type="hidden" id="tt_cnt" name="tt_cnt"
					value="${fn:length(list) }">
				<c:forEach items="${list}" var="vo">
					<p
						style="font-weight: bold; font-size: 20px; font-family: monospace;">${vo.no }번.
						${vo.exam_content }</p>
					<c:if test="${vo.exam_cnt eq 0 }">
						<div class="form-floating mb-3">
							<input class="form-control" name="${vo.no}"
								id="${vo.no}
								placeholder=" Enter your
								name..." data-sb-validations="required" /> <label for="name">답안작성</label>
							<div class="invalid-feedback" data-sb-feedback="name:required">req</div>
						</div>

					</c:if>
					<c:if test="${vo.exam_cnt ne 0 }">
						<div style="margin: 50px auto;">

							<c:forEach var="i" begin="1" end="${vo.exam_cnt }">

								<label class="box-radio-input"> <input type="radio"
									name="${vo.no }" id="${vo.no }" value="${i}"> <span>${i}</span></label>

							</c:forEach>
							</div>
					</c:if>
				</c:forEach>
				<input class="wpcf7-submit" type="submit" value="전송">
			</form>
		</div>
